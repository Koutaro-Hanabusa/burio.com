{
	"extends": "../tsconfig.json",
	"compilerOptions": {
		"jsx": "react",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"types": ["@cloudflare/workers-types"],
		"lib": ["ES2020"],
		"target": "ES2020",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"esModuleInterop": true,
		"skipLibCheck": true,
		"strict": true,
		"noEmit": true
	},
	"include": ["**/*.ts", "**/*.tsx", "types.d.ts"]
}
